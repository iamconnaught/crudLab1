    <head>
        <meta charset="utf-8">
        <title></title>
    </head>
    <body>
        <h1>Car Index Page</h1>
        <ul>
            <% for(let i = 0; i < cars.length; i++){ %>
                <li>
                    Make: <a href="/cars/<%=cars[i].id%>"><%=cars[i].make; %></a><br/>
                    Model: <%=cars[i].model; %>

                    <form action="/cars/<%=cars[i]._id%>?_method=DELETE" method="POST">
                      <button type="submit">Delete</button>
                    </form>
                    <a href="/cars/<%=cars[i]._id%>/edit">(EDIT)</a>
                </li>
            <% } %>
        </ul>
        <nav>
            <a href="/cars/new">Create a New Car</a>
        </nav>
    </body>
</html>